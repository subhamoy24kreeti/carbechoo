
<div class="container">
<% if flash.key? :alert %>
<p><%= flash[:alert] %></p>
<% end %>
<%= form_with url: seller_create_path, method: :post do |f| %>
    <div class="form-group">
      <%= f.label 'first name' ,class:"form-lebel"%>
      <%= f.text_field :first_name, required: true ,class: "form-control"%>
    </div>
    <div class="form-group">
      <%= f.label 'last name' ,class:"form-lebel"%>
      <%= f.text_field :last_name, required: true ,class: "form-control"%>
    </div>
    <div class="form-group">
      <%= f.label :email ,class:"form-lebel"%>
      <%= email_field_tag :email, "", required: true ,class: "form-control"%>
    </div>
  
    <div class="form-group">
      <%= f.label :password  ,class:"form-lebel"%>
      <%= password_field_tag :password, "", required: true, class: "form-control" %>
      
    </div>

    <div class="form-group">
      <%= f.label 'confirm password'  ,class:"form-lebel"%>
      <%= password_field_tag :password_confirmation, "", required: true, class: "form-control" %>
    </div>
  
    <div class="actions">
      <%= f.submit "Sign up" ,class: "btn btn-primary w-100"%>
    </div>
  <% end %>
  </div>