<div class="container">
<%= form_with url: user_login_post_path, method: :post do |f| %>
<fieldset class="form-group">
<%= f.label :email %>
<%= f.text_field :email, class:'form-control'%>
</fieldset>
<fieldset>
<%= f.label :password %>
<%= f.text_field :password, type: "password", class:"form-control"%>
</fieldset>
<div class="form-group">
<%= f.submit 'login', class:"btn btn-primary mt-3" %>
</div>
<% end %>
</div>