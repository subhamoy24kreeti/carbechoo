
<div class="wrapper">
  <%= render "admin/layouts/sidebar" %>
  <div class='container-fluid p-0 mt-3'>
   <%= render "admin/layouts/header" %>
  <section>
  <div class="container pt-3">
    <% if flash.key? :notice %>
  <%= render 'layouts/alert_success', notice: flash[:notice] %>
  <% end %>

  <% if flash.key? :error %>
  <%= render 'layouts/alert_error', error: flash[:error]%>
  <% end %>
    <%= form_with url: admin_car_registration_year_create_path,  method: :post do |f|%>
    <div class="form-group">
     <%=f.label :range1 %>
     <%=f.text_field :range1, class:"form-control" %>
    </div>
    <div class="form-group">
    <%=f.label :range2 %>
    <%=f.text_field :range2, class:"form-control" %>
     </div>
    <%=f.submit :add ,class:"btn btn-info" %>
    <% end%>
    </div>
  </section>
  </div>
  </div>