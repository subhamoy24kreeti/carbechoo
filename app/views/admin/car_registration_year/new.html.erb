
<div class="wrapper">
    <%= render "admin/layouts/sidebar" %>
    <div class='container-fluid p-0'>
       <%= render "admin/layouts/header" %>
        <section>
        <% if flash.key? :alert %>
        <p><%= flash[:alert] %></p>
        <% end %>
        <div class="container pt-3">
          <%= form_with url: admin_car_registration_year_create_path,  method: :post do |f|%>
            <div class="form-group">
             <%=f.label :range1 %>
             <%=f.text_field :range1, class:"form-control" %>
            </div>
            <div class="form-group">
              <%=f.label :range2 %>
              <%=f.text_field :range2, class:"form-control" %>
             </div>
            <%=f.submit :add ,class:"btn btn-info" %>
          <% end%>
          </div>
        </section>
    </div>
    </div>